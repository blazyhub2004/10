\documentclass{article}
\usepackage{tikz}
\begin{document}
\centering
\tikzstyle{level 1}=[level distance=4cm, sibling distance=4cm]
\tikzstyle{level 2}=[level distance=4cm, sibling distance=2cm]
\begin{tikzpicture}[grow=down, sloped]
    \node {Rooot}
    child {
        node {Child 1}
        child {
            node {Subchild 1}
        }
        child {
            node {Subchild 2}
        }
    }
    child {
        node {Child 2}
        child {
            node {Subchild 1}
        }
        child {
            node {Subchild 2}
        }
    };
\end{tikzpicture}

\end{document}
